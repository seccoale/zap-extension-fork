<zapaddon>
	<name>Script Console</name>
	<version>9</version>
	<description>Supports all JRS 223 scripting languages</description>
	<author>ZAP Dev Team</author>
	<url>https://code.google.com/p/zaproxy/wiki/ScriptConsole</url>
	<changes>Moved script support into the core and added targeted scripts</changes>
	<dependson/>
	<extensions>
		<extension>org.zaproxy.zap.extension.scripts.ExtensionScripts</extension>
	</extensions>
	<ascanrules>
		<ascanrule>org.zaproxy.zap.extension.scripts.ScriptsActiveScanner</ascanrule>
	</ascanrules>
	<pscanrules>
		<pscanrule>org.zaproxy.zap.extension.scripts.ScriptsPassiveScanner</pscanrule>
	</pscanrules>
	<filters/>
	<files/>
	<not-before-version>2.2.0</not-before-version>
	<not-from-version/>
</zapaddon>
